@model IEnumerable<ASP.NET_MVC_Tutorial.Models.Employee>

@using ASP.NET_MVC_Tutorial.Models;

@{
    ViewBag.Title = "Employee List";
}

<div style="font-family:Arial">
    <h2>@ViewBag.Title</h2>
    <ul>
        @foreach(Employee employee in @Model)
        {
            <li>
                @Html.ActionLink(employee.Name, "Details", new { id = employee.Id })
            </li>
        }
    </ul>
    @Html.ActionLink("Back to Department List", "Index", "Department")
</div>



